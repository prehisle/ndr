# 应用服务配置
APP_IMAGE=ndr:prod
APP_PORT=9000
COMPOSE_PROJECT_NAME=ndr_prod_9000

# API 应用环境变量
# DB_URL 中的密码必须与 POSTGRES_PASSWORD 保持一致
DB_URL=postgresql+psycopg2://ndr:ndr@db:5432/ndr
DB_CONNECT_TIMEOUT=5
ENABLE_METRICS=true
API_KEY_ENABLED=false
API_KEY=
DESTRUCTIVE_API_KEY=
CORS_ENABLED=false
CORS_ORIGINS=
AUTO_APPLY_MIGRATIONS=true
TRACE_HTTP=false

# PostgreSQL 容器配置
POSTGRES_DB=ndr
POSTGRES_USER=ndr
POSTGRES_PASSWORD=请修改为更安全的密码
POSTGRES_HOST_PORT=5432
